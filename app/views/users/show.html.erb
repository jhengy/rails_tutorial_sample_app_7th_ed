<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
      <%= "Current Time: #{Time.now}"  %>
      <%= "Created At: #{@user.created_at}"  %>
      <%= "Updated At: #{@user.updated_at}"  %>
    </section>
  </aside>
</div>
